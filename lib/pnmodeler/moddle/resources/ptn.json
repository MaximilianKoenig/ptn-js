{
  "name": "Place Transition nets",
  "uri": "http://bpt-lab.org/schemas/ptn",
  "prefix": "ptn",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "AbstractElement",
      "isAbstract": true,
      "properties": [
        {
          "name": "id",
          "isAttr": true,
          "type": "String",
          "isId": true
        },
        {
          "name": "name",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    {
      "name": "ModelElement",
      "isAbstract": true,
      "superClass": ["AbstractElement"]
    },
    {
      "name": "ModelNode",
      "isAbstract": true,
      "superClass": ["ModelElement"]
    },
    {
      "name": "Place",
      "superClass": ["ModelNode"],
      "properties": [
        {
          "name": "marking",
          "isAttr": true,
          "type": "Integer",
          "default": 0
        }
      ]
    },
    {
      "name": "Transition",
      "superClass": ["ModelNode"],
      "properties": []
    },
    {
      "name": "ModelConnection",
      "isAbstract": true,
      "superClass": ["ModelElement"]
    },
    {
      "name": "Arc",
      "superClass": ["ModelConnection"],
      "properties": [
        {
          "name": "source",
          "isAttr": true,
          "type": "ModelNode",
          "isReference": true
        },
        {
          "name": "target",
          "isAttr": true,
          "type": "ModelNode",
          "isReference": true
        },
        {
          "name": "weight",
          "isAttr": true,
          "type": "String"
        }
      ]
    },
    {
      "name": "Model",
      "superClass": ["AbstractElement"],
      "properties": [
        {
          "name": "modelElements",
          "type": "ModelElement",
          "isMany": true
        }
      ]
    },
    {
      "name": "Definitions",
      "superClass": ["AbstractElement"],
      "properties": [
        {
          "name": "targetNamespace",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "expressionLanguage",
          "default": "http://www.w3.org/1999/XPath",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "typeLanguage",
          "default": "http://www.w3.org/2001/XMLSchema",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "model",
          "type": "Model"
        },
        {
          "name": "diagram",
          "type": "ptnDi:Diagram"
        },
        {
          "name": "exporter",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "exporterVersion",
          "isAttr": true,
          "type": "String"
        }
      ]
    }
  ]
}
