{
  "name": "Petri net Diagrams",
  "uri": "http://bpt-lab.org/schemas/ptnDi",
  "prefix": "ptnDi",
  "xml": {
    "tagAlias": "lowerCase"
  },
  "types": [
    {
      "name": "Diagram",
      "properties": [
        {
          "name": "plane",
          "type": "Plane",
          "redefines": "AbstractDiagram#rootElement"
        }
      ],
      "superClass": ["AbstractDiagram"]
    },
    {
      "name": "AbstractDiagram",
      "isAbstract": true,
      "properties": [
        {
          "name": "id",
          "isAttr": true,
          "isId": true,
          "type": "String"
        },
        {
          "name": "name",
          "isAttr": true,
          "type": "String"
        },
        {
          "name": "rootElement",
          "type": "DiagramElement",
          "isReadOnly": true,
          "isVirtual": true
        }
      ]
    },
    {
      "name": "Plane",
      "properties": [
        {
          "name": "modelElement",
          "isAttr": true,
          "isReference": true,
          "type": "ptn:AbstractElement"
        }
      ],
      "superClass": ["AbstractPlane"]
    },
    {
      "name": "AbstractPlane",
      "isAbstract": true,
      "properties": [
        {
          "name": "planeElement",
          "type": "DiagramElement",
          "subsettedProperty": "DiagramElement-ownedElement",
          "isMany": true
        }
      ],
      "superClass": ["DiagramElement"]
    },
    {
      "name": "DiagramShape",
      "properties": [
        {
          "name": "modelElement",
          "isAttr": true,
          "isReference": true,
          "type": "ptn:AbstractElement"
        },
        {
          "name": "label",
          "type": "DiagramLabel"
        }
      ],
      "superClass": ["LabeledShape"]
    },
    {
      "name": "LabeledShape",
      "isAbstract": true,
      "properties": [
        {
          "name": "ownedLabel",
          "type": "Label",
          "isReadOnly": true,
          "subsettedProperty": "DiagramElement-ownedElement",
          "isMany": true,
          "isVirtual": true
        }
      ],
      "superClass": ["Shape"]
    },
    {
      "name": "Shape",
      "isAbstract": true,
      "properties": [
        {
          "name": "bounds",
          "type": "dc:Bounds"
        }
      ],
      "superClass": ["DiagramElement"]
    },
    {
      "name": "DiagramEdge",
      "properties": [
        {
          "name": "modelElement",
          "isAttr": true,
          "isReference": true,
          "type": "ptn:AbstractElement"
        },
        {
          "name": "label",
          "type": "DiagramLabel"
        },
        {
          "name": "sourceElement",
          "isAttr": true,
          "isReference": true,
          "type": "ptn:AbstractElement",
          "redefines": "Edge#source"
        },
        {
          "name": "targetElement",
          "isAttr": true,
          "isReference": true,
          "type": "ptn:AbstractElement",
          "redefines": "Edge#target"
        }
      ],
      "superClass": ["LabeledEdge"]
    },
    {
      "name": "LabeledEdge",
      "isAbstract": true,
      "properties": [
        {
          "name": "ownedLabel",
          "type": "Label",
          "isReadOnly": true,
          "subsettedProperty": "DiagramElement-ownedElement",
          "isMany": true,
          "isVirtual": true
        }
      ],
      "superClass": ["Edge"]
    },
    {
      "name": "Edge",
      "isAbstract": true,
      "properties": [
        {
          "name": "source",
          "type": "ptn:AbstractElement",
          "isReadOnly": true,
          "isVirtual": true,
          "isReference": true
        },
        {
          "name": "target",
          "type": "ptn:AbstractElement",
          "isReadOnly": true,
          "isVirtual": true,
          "isReference": true
        },
        {
          "name": "waypoint",
          "type": "dc:Point",
          "isUnique": false,
          "isMany": true,
          "xml": {
            "serialize": "xsi:type"
          }
        }
      ],
      "superClass": ["DiagramElement"]
    },
    {
      "name": "DiagramLabel",
      "superClass": ["Label"]
    },
    {
      "name": "Label",
      "isAbstract": true,
      "properties": [
        {
          "name": "bounds",
          "type": "dc:Bounds"
        }
      ],
      "superClass": ["DiagramElement"]
    },
    {
      "name": "DiagramElement",
      "isAbstract": true,
      "properties": [
        {
          "name": "id",
          "isAttr": true,
          "isId": true,
          "type": "String"
        },
        {
          "name": "owningDiagram",
          "type": "AbstractDiagram",
          "isReadOnly": true,
          "isVirtual": true,
          "isReference": true
        },
        {
          "name": "owningElement",
          "type": "DiagramElement",
          "isReadOnly": true,
          "isVirtual": true,
          "isReference": true
        }
      ]
    }
  ]
}